{
  "name": "xnxktech/laravel-evidence",
  "description": "Provide eSign evidence API for Laravel",
  "type": "library",
  "require": {
    "php": "^8.0",
    "ext-openssl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^8.0 || ^9.0",
    "monolog/monolog": "^2.0",
    "pimple/pimple": "^3.0",
    "symfony/http-foundation": "^5.4 || ^6.0",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.12.0",
    "illuminate/support": "^9.3",
    "mockery/mockery": "1.5.1",
    "nunomaduro/phpinsights": "2.6.1",
    "orchestra/testbench": "8.2.1",
    "pestphp/pest": "1.22.1",
    "pestphp/pest-plugin-parallel": "1.2.0",
    "phpunit/phpunit": "9.5.25"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "YGNICE",
      "email": "ygnice888@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "XNXK\\LaravelEvidence\\": "src/"
    },
    "files":[
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "XNXK\\LaravelEvidence\\ServiceProvider"
      ]
    }
  }
}
